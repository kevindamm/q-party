{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "./types/workers.d.ts",
      "./types/files.d.ts",
      "@cloudflare/workers-types"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "exclude": [
    "node_modules",
    "test/**/*.ts",
    "**/*.test.ts"
  ],
  "include": [
    "types/**/*.d.ts",
    "htmx/**/*.html",
    "src/**/*.ts"
  ]
}